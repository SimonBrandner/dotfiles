# GIMP gimprc
# 
# This is your personal gimprc file.  Any variable defined in this file takes
# precedence over the value defined in the system-wide gimprc:
# /etc/gimp/2.0/gimprc
# Most values can be set within GIMP by changing some options in the
# Preferences dialog.

(use-opencl yes)
(language "en_US")
(default-image
    (width 1920)
    (height 1080)
    (unit pixels)
    (xresolution 300)
    (yresolution 300)
    (resolution-unit inches)
    (image-type rgb)
    (precision float-linear)
    (color-managed yes)
    (color-profile NULL)
    (fill-type transparent)
    (comment "Created with GIMP"))
(import-raw-plug-in "${gimp_plug_in_dir}/plug-ins/file-darktable/file-darktable")
(transparency-type dark-checks)
(snap-distance 16)
(default-view
    (show-menubar yes)
    (show-statusbar yes)
    (show-rulers yes)
    (show-scrollbars yes)
    (show-selection yes)
    (show-layer-boundary yes)
    (show-canvas-boundary yes)
    (show-guides yes)
    (show-grid no)
    (show-sample-points yes)
    (snap-to-guides yes)
    (snap-to-grid no)
    (snap-to-canvas yes)
    (snap-to-path no)
    (padding-mode default)
    (padding-color (color-rgb 0.36078399999999999 0.35795500000000002 0.35795500000000002))
    (padding-in-show-all no))
(default-fullscreen-view
    (show-menubar yes)
    (show-statusbar yes)
    (show-rulers yes)
    (show-scrollbars yes)
    (show-selection yes)
    (show-layer-boundary yes)
    (show-canvas-boundary yes)
    (show-guides yes)
    (show-grid no)
    (show-sample-points yes)
    (snap-to-guides yes)
    (snap-to-grid no)
    (snap-to-canvas yes)
    (snap-to-path no)
    (padding-mode default)
    (padding-color (color-rgb 0.42745100000000003 0.42745100000000003 0.42745100000000003))
    (padding-in-show-all no))
(move-tool-changes-active yes)
(toolbox-wilber no)
(theme "Gimp-Arc-Dark-Theme")
(icon-theme "Color")
(icon-size theme)
(user-manual-online yes)
(image-convert-precision-text-layer-dither-method floyd-steinberg)
(layer-new-name "1 Paint")
(layer-new-fill-type foreground)
(fill-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10))
(stroke-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10)
    (method line)
    (width 6)
    (unit pixels)
    (cap-style butt)
    (join-style miter)
    (miter-limit 10)
    (dash-offset 0)
    (dash-info 0)
    (emulate-brush-dynamics no))


# end of gimprc
